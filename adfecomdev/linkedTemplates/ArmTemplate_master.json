{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfecomdev"
		},
		"AzureDataLakeStorage_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_ls'"
		},
		"AzureDataLakeStorage_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://formula2adls.dfs.core.windows.net/"
		},
		"AzureKeyVault_ls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://sqlserverdb-key.vault.azure.net/"
		},
		"AzureSqlDatabase_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "@{linkedService().qualifieddomainname}"
		},
		"AzureSqlDatabase_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().databasename}"
		},
		"AzureSqlDatabase_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "@{linkedService().username}"
		},
		"event_rawecomdadls_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/067e3b03-4f76-4f0c-ac2c-dc646ae2f9ac/resourceGroups/rg-databricks/providers/Microsoft.Storage/storageAccounts/formula2adls"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfecomdev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_ls_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_ls_accountKey')]"
					},
					"AzureDataLakeStorage_ls_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_ls_properties_typeProperties_url')]"
					},
					"AzureKeyVault_ls_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_ls_properties_typeProperties_baseUrl')]"
					},
					"event_rawecomdadls_properties_typeProperties_scope": {
						"value": "[parameters('event_rawecomdadls_properties_typeProperties_scope')]"
					},
					"AzureSqlDatabase_ls_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_ls_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_ls_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_ls_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase_ls_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}